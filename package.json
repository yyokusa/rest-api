{
  "name": "backend-foodimium",
  "version": "1.0.0",
  "description": "backend for the foodimium application",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "debug": "export DEBUG=* && npm run start",
    "test": "mocha -r ts-node/register 'test/**/*.test.ts' --unhandled-rejections=strict --bail",
    "test-debug": "export DEBUG=* && npm test",
    "test-debug2": "DEBUG=mquery npm run test",
    "start": "node --unhandled-rejections=strict ./dist/index.js",
    "build": "rm -rf node_modules && npm i && npm install sqlite3 && rimraf dist && ./node_modules/typescript/bin/tsc && cp ./node_modules/sqlite3/lib/binding/napi-v6-darwin-unknown-arm64/node_sqlite3.node ./dist/ && cp .env ./dist/.env && npm rebuild",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/foodimium/backend-foodimium.git"
  },
  "author": "yyokusa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/foodimium/backend-foodimium/issues"
  },
  "homepage": "https://github.com/foodimium/backend-foodimium#readme",
  "dependencies": {
    "argon2": "^0.30.3",
    "axios": "^1.3.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "express-winston": "^4.2.0",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.0",
    "sequelize": "^6.31.0",
    "shortid": "^2.2.16",
    "sqlite3": "^5.1.6",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mocha": "^10.0.1",
    "@types/shortid": "^0.0.29",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "chai": "^4.3.7",
    "eslint": "^8.38.0",
    "mocha": "^10.2.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.0.4"
  }
}
